{
  "manifest_version": 2,
  "content_scripts": [{
    "include_globs": [
      "http://*.facebook.com/*",
      "http://facebook.com/*",
      "https://*.facebook.com/*",
      "https://facebook.com/*"
    ],
    "exclude_globs": [
      "http://*.channel.facebook.com/*",
      "http://static.*.facebook.com/*",
      "http://*.facebook.com/ai.php*",
      "http://*.facebook.com/connect/*",
      "https://*.channel.facebook.com/*",
      "https://static.*.facebook.com/*",
      "https://*.facebook.com/ai.php*",
      "https://*.facebook.com/connect/*",
      "https://*.facebook.com/saved*"
    ],
    "js": [
      "js/script.js",
      "js/jquery.js",
      "js/fx.js"
    ],
    "css": [
    ],
    "matches": [
      "http://*.facebook.com/*",
      "https://*.facebook.com/*"
    ],
    "run_at": "document_start"
  }],
  "name": "Digital Literacy Facebook Game",
  "description": "This extension adds a guess who box game to each sharer on Facebook",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}